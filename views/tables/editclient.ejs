<%- include('../layout/head.ejs')%>
<header class="header">
    <%- include('../layout/header.ejs')%>
    <%- include('../layout/nav.ejs')%>
</header>

<main class="main">
    <section class="edittown">
        <div class="container">
            <div class="edittown__back client-pith__href">
                <% for(var i = 0; i < posts.length; i++) { %>
                    <% if(posts[i].id == clients.post) { %>
                        <a href="/tables/<%=posts[i].url %>" class="edittown__back--link">Повернутися назад</a> 
                    <% } %>
                <% } %>
            </div>
             <h3 class="edittown__name" id="<%=clients.id %>"><span class="edittown__name--span">Назва міста:</span> <%= clients.headline %></h3>
             <div class="edittonw__box">
                    <h3 class="edittown__box--name">Назва міста:</h3>
                    <input type="text" class="edittown__box--input" value="<%=clients.headline %>">
             </div>
             <div class="edittown__button">
                <button class="editclient__button--click button">Застосувати зміни</button>
             </div>
        </div>
    </section>
</main>
<%- include('../layout/footer.ejs')%>