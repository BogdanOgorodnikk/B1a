    <%- include('../layout/header.ejs')%>
    <%- include('../layout/nav.ejs')%>
</header>

<main class="main">
    <section class="productnameedit">
        <div class="container">
            <div class="productnameedit__box">
                <h3 class="numberedit__text" id="<%= numbers.id %>">Кількість тон</h3>
                <div class="productnameedit__line">
                    <input type="text" class="numberedit__line--input" value="<%= numbers.number %>">
                </div>
            </div>
            <div class="productnameedit__button">
                <button class="numberedit__button--btn">Змінити</button>
            </div>
        </div>
    </section>
</main>
<%- include('../layout/footer.ejs')%>